<%= form_for @venue do |f| %>
   <%= f.label :name %>
   <%= f.text_field :name %><br>
   <%= f.label :location %>
   <%= f.text_field :location %><br>
   <%= f.label :description %>
   <%= f.text_field :description %><br>
   <%= f.label :category %>
   <%= f.text_field :category %><br>
   <%= f.label :capacity %>
   <%= f.number_field :capacity %><br>
   <%=f.file_field :picture%><br>
   <%= f.submit %>
<% end %>



