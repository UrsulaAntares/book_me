<%= form_for @venue do |f| %>
   <%= f.label :name %>
   <%= f.text_field :name %><br>
   <%= f.label :location %>
   <%= f.text_field :location %><br>
   <%= f.label :description %>
   <%= f.text_field :description %><br>
   <%= f.label :category %>
   <%= f.text_field :category %><br>
   <%= f.label :capacity %>
   <%= f.number_field :capacity %><br>
   <%= f.file_field :picture%><br>
   <%= f.submit %>
<% end %>


<% if @venue.users.count > 0 %>
   Current Users with admin rights:
   <% @venue.users.each do |user|%>
         <%=user.username%>
   <%end%>
<%end %>
<hr>

<%current_user.venue_admin(@venue).each do |user|%> 
    <%=user.username%>
<%end%>