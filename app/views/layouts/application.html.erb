<!DOCTYPE html>
<html>
  <head>
    <title>BookMe</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  
  <nav class="navbar">
  <a class="navbar-brand" href="/artists">Artists</a>  | 
  <a class="navbar-brand" href="/venues">Venues</a>  | 
  <a class="navbar-brand" href="/bookings">Calendar</a> 
</nav>

<div>
<%if !current_user%>
<%=link_to "Login", login_path%>
<%end%>

<%if current_user%>
<%=link_to "Logout", logout_path, method: 'destroy'%>
<%=link_to "Dashboard", user_path(current_user)%>
<%end%>
</div>




  <body>
    <%= yield %>
  </body>
</html>


